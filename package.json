{
  "name": "react-activity-feed",
  "version": "1.0.0",
  "description": "React components to create activity and notification feeds",
  "author": "GetStream",
  "license": "BSD-3-Clause",
  "repository": "GetStream/react-activity-feed",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "keywords": [
    "feed",
    "newsfeed",
    "activity stream",
    "react",
    "notification feed",
    "stream",
    "getstream",
    "getstream.io"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "eslint": "eslint '**/*.js' --max-warnings 0",
    "flow": "flow",
    "prettier": "prettier --list-different '**/*.{js,ts}'",
    "lint": "yarn prettier && yarn eslint",
    "lint-fix": "prettier --write '**/*.{js,ts}' && eslint --fix '**/*.js'",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "docs": "styleguidist build",
    "docs-server": "styleguidist server",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^8.2.5",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.80.0",
    "gh-pages": "^1.2.0",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^1.1.4",
    "react-styleguidist": "^7.3.7",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "emoji-mart": "^2.8.1",
    "getstream": "^3.21.0",
    "image-exists": "^1.1.0",
    "immutable": "^4.0.0-rc.9",
    "is-retina": "^1.0.3",
    "moment": "^2.22.2",
    "postcss-color-function": "^4.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-simple-vars": "^5.0.1",
    "remove": "^0.1.5",
    "stream-analytics": "^2.7.1"
  }
}
